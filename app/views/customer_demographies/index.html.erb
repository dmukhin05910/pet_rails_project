<p id="notice"><%= notice %></p>

<h1>Customer Demographies</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customer_demographies.each do |customer_demography| %>
      <tr>
        <td><%= link_to 'Show', customer_demography %></td>
        <td><%= link_to 'Edit', edit_customer_demography_path(customer_demography) %></td>
        <td><%= link_to 'Destroy', customer_demography, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Customer Demography', new_customer_demography_path %>
